<?xml version="1.0"?>
<grammar type="window" version="1.0" author="Gen Nishida">
	<rule name="WindowGlass">
		<texture texturePath="textures/window_glass.png"/>
	</rule>
	
	<rule name="WindowGlass_06">
		<setupProjection axesSelector="scope.xy">
			<param name="texWidth" type="relative" value="1"/>
			<param name="texHeight" type="relative" value="1"/>
		</setupProjection>
		<texture texturePath="textures/window.9.tif"/>
	</rule>
	
	<rule name="WindowGlass_08">
		<setupProjection axesSelector="scope.xy">
			<param name="texWidth" type="relative" value="1"/>
			<param name="texHeight" type="relative" value="1"/>
		</setupProjection>
		<texture texturePath="textures/window.9.tif"/>
	</rule>
	
	<rule name="Door">
		<color s="#000000"/>
	</rule>
</grammar>

