<?xml version="1.0"?>
<grammar type="floor" version="1.0" author="Gen Nishida">
	<rule name="Start">
		<split splitAxis="x">
			<param type="floating" value="4" name="FloorColumn_03" repeat="true"/>
			<param type="floating" value="4" name="EntranceColumn_03"/>
		</split>
	</rule>
	<rule name="FloorColumn_03">
		<split splitAxis="x">
			<param type="relative" value="0.22" name="Wall"/>
			<param type="relative" value="0.56" name="FloorColumn2_03"/>
			<param type="relative" value="0.22" name="Wall"/>
		</split>
	</rule>
	<rule name="FloorColumn2_03">
		<split splitAxis="y">
			<param type="relative" value="0.32" name="Wall"/>
			<param type="relative" value="0.5" name="Window_03"/>
			<param type="relative" value="0.18" name="Wall"/>
		</split>
	</rule>
	<rule name="EntranceColumn_03">
		<split splitAxis="x">
			<param type="relative" value="0.22" name="Wall"/>
			<param type="relative" value="0.56" name="EntranceColumn2_03"/>
			<param type="relative" value="0.22" name="Wall"/>
		</split>
	</rule>
	<rule name="EntranceColumn2_03">
		<split splitAxis="y">
			<param type="relative" value="0.82" name="Entrance_03"/>
			<param type="relative" value="0.18" name="Wall"/>
		</split>
	</rule>
</grammar>

