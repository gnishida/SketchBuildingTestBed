<?xml version="1.0"?>
<grammar type="floor" version="1.0" author="Gen Nishida">
	<rule name="Start">
		<split splitAxis="x">
			<param type="floating" value="6" name="FloorColumn_04" repeat="true"/>
			<param type="floating" value="4" name="EntranceColumn_04"/>
		</split>
	</rule>
	<rule name="FloorColumn_04">
		<split splitAxis="x">
			<param type="relative" value="0.16" name="Wall"/>
			<param type="relative" value="0.16" name="FloorColumn2_04"/>
			<param type="relative" value="0.16" name="Wall"/>
			<param type="relative" value="0.36" name="FloorColumn3_04"/>
			<param type="relative" value="0.16" name="Wall"/>
		</split>
	</rule>
	<rule name="FloorColumn2_04">
		<split splitAxis="y">
			<param type="relative" value="0.32" name="Wall"/>
			<param type="relative" value="0.5" name="Window_04"/>
			<param type="relative" value="0.18" name="Wall"/>
		</split>
	</rule>
	<rule name="FloorColumn3_04">
		<split splitAxis="y">
			<param type="relative" value="0.32" name="Wall"/>
			<param type="relative" value="0.5" name="Window2_04"/>
			<param type="relative" value="0.18" name="Wall"/>
		</split>
	</rule>
	<rule name="EntranceColumn_04">
		<split splitAxis="x">
			<param type="relative" value="0.22" name="Wall"/>
			<param type="relative" value="0.56" name="EntranceColumn2_04"/>
			<param type="relative" value="0.22" name="Wall"/>
		</split>
	</rule>
	<rule name="EntranceColumn2_04">
		<split splitAxis="y">
			<param type="relative" value="0.82" name="Entrance_04"/>
			<param type="relative" value="0.18" name="Wall"/>
		</split>
	</rule>
</grammar>

